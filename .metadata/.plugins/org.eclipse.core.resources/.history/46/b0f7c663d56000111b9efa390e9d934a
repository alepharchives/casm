#include <stdlib.h>
#include "DoubleLinkedList.h"

List* mkList() {
	return (List*) malloc(sizeof(List));
}

void addLast(List** l, void* data) {
	while (*l->next!=NULL)
		*l=l->next;

	*l->next = (List*) malloc(sizeof(List));
	*l->next->prev=l;
}

List* append(void* head, List* list) {
	List* h = (List*) malloc(sizeof(List));
	h->data= head;
	h->prev = list;
	list->next = h;
	return h;
}

