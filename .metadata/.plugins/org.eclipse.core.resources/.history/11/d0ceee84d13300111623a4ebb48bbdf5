test: all.o
	gcc -o all.o
	
all.o: mat.c mat.h parser.c parser.h matdsl.c matdsl.h main.c
	gcc -ansi -pedantic -Wall -g -c mat.c parser.c matdsl.c main.c -o all.o

interMat: mat.o parser.o matdsl.o main.o 
	gcc -o mat.o matdsl.o parser.o main.o

mat.o: mat.c mat.h
	gcc -ansi -pedantic -Wall -g -c mat.c

parser.o: parser.c parser.h
	gcc -ansi -pedantic -Wall -g -c parser.c	

matdsl.o: matdsl.c matdsl.h  
	gcc -ansi -pedantic -Wall -I. -g -c matdsl.c	

main.o: main.c 
	gcc -ansi -pedantic -Wall -g -c main.c 

clean:
	rm -rf *.o